{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "15 11 * * *"
        }
    ]
}